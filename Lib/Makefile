# Build all libraries

MAKEDEFS = FLOAT_TYPE=$(FLOAT_TYPE) BINPATH=$(BINPATH) DEVICE_DEF=$(DEVICE_DEF) BASEDIR=$(BASEDIR) OUTPATH=$(OUTPATH)

all:
	$(MAKE) -C STM32F2xx_StdPeriph_Driver $(MAKEDEFS)
	$(MAKE) -C STM32F2x7_ETH_Driver $(MAKEDEFS)
	$(MAKE) -C CMSIS $(MAKEDEFS)
	
clean:
	$(MAKE) clean -C STM32F2xx_StdPeriph_Driver $(MAKEDEFS)
	$(MAKE) clean -C STM32F2x7_ETH_Driver $(MAKEDEFS)
